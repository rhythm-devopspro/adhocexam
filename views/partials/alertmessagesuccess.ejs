<% if (errors.length > 0) { %>
<% errors.forEach(function(error){ %>
<div class="alert alert-<%=alertType%> alert-dismissible fade show" role="alert">
  <%=error.message%>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %>
<% } %>
