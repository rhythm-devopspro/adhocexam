
<% if (foundLab && foundLab.length>0){ %>
  <% foundLab.forEach(function(lab){ %>
    <div class="xl:w-1/4 md:w-1/2 p-4">
      <div class="bg-gray-100 p-6 rounded-lg">
        <img class="h-40 rounded w-full object-cover object-center mb-6" src="<%=lab.thumbnailUrl%>" alt="<%=lab.catagory%>">
        <h3 class="tracking-widest text-blue-500 text-xs font-medium title-font text-uppercase"><%=lab.catagory%></h3>
        <h2 class="text-lg text-gray-900 font-medium title-font mb-4 text-uppercase"><%=lab.labName%>  </h2>
        <p class="leading-relaxed text-base"><%=lab.labDescription%></p>
        <a href="/practice/<%=lab._id%>" class="btn btn-secondary mt-3">Start Lab</a>
      </div>
    </div>
  <% }); %>
<% } %>
