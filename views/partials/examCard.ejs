<% if (examFoundLabs.length > 0) { %>
  <% examFoundLabs.forEach(function(examFoundLab){ %>
    <div class="xl:w-1/4 md:w-1/2 p-4">
      <div class="bg-gray-100 p-6 rounded-lg">
        <img class="h-40 rounded w-full object-cover object-center mb-6" src="<%=examFoundLab.thumbnailUrl%>" alt="content">
        <h3 class="tracking-widest text-blue-500 text-xs font-medium title-font text-uppercase"><%=examFoundLab.catagory%></h3>
        <h2 class="text-lg text-gray-900 font-medium title-font mb-4 text-uppercase"><%=examFoundLab.labName%></h2>
        <p class="leading-relaxed text-base"><%=examFoundLab.examDetails%></p>
        <!-- <a href="/questions" class="btn btn-secondary mt-3">Start Exam</a> -->
        <button type="button" class="btn btn-secondary mt-3" data-toggle="modal" data-target="#startExamModel" data-exam-id="<%=examFoundLab._id%>">Start Exam</button>
      </div>
    </div>
 <% }); %>
<% } %>
